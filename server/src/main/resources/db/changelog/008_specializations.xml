<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

    <changeSet  id="008-01"  author="omaraalsaied">
        <createTable tableName="specializations">
            <column name="id" type="int(11)" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" />
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false" />
            </column>
        </createTable>

        <insert tableName="specializations">
            <column name="name" value="surgery"/>
        </insert>

        <insert tableName="specializations">
            <column name="name" value="Dermatology" />
        </insert>

        <insert tableName="specializations">
            <column name="name" type="Pediatrics" />
        </insert>

    </changeSet>

</databaseChangeLog>